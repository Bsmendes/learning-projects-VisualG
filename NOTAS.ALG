Algoritmo "notas"
Var
     primeiraNota, segundaNota, final : real
     


Inicio
   escreval("Digite a primeira nota : ")
   leia(primeiraNota)
   escreval("difite a segunda nota : ")
   leia(segundaNota)
   final <- primeiraNota + segundaNota
   se (final) <60 entao
      escreval("Reprovado")

   senao
      escreval(" Nota final =",final)
   
   fimse




Fimalgoritmo
